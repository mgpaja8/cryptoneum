{
  "type": "sqlite",
  "database": "db.sqlite",
  "synchronize": true,
  "logging": true,
  "migrationsRun": true,
  "entities": [
     "src/models/**/*.ts"
  ],
  "migrations": [
     "src/migration/**/*.ts"
  ],
  "subscribers": [
     "src/subscriber/**/*.ts"
  ],
  "cli": {
     "entitiesDir": "src/models",
     "migrationsDir": "src/migration",
     "subscribersDir": "src/subscriber"
  }
}
